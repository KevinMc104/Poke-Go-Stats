<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.pokemon.detailed.PokemonDetailedWeatherListFragment">

    <com.example.pokegostats.view.custom.PokemonDetailedRowView
        android:id="@+id/weather_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <com.example.pokegostats.view.custom.PokemonDetailedRowView
        android:id="@+id/weather_name2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weather_name"/>

    <com.example.pokegostats.view.custom.PokemonDetailedRowView
        android:id="@+id/weather_name3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weather_name2"/>


</androidx.constraintlayout.widget.ConstraintLayout>
